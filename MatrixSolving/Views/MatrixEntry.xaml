<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MatrixSolving.Views.MatrixEntry">
    <Grid>
        <Label x:Name="cellText" FontSize="20" VerticalOptions="Center" MaxLines="1"/>
        <Button x:Name="cellButton" BackgroundColor="Transparent" CornerRadius="4" BorderWidth="1" Pressed="CellClicked"
                BorderColor="Gray"/>
    </Grid>
</ContentView>
